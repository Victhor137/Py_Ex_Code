# Input
total_eleitores = int(input("Qual é o total de eleitores? "))
votos_brancos = int(input("Quantidade de votos brancos? "))
votos_nulos = int(input("Quantidade de votos nulos? "))
votos_validos = int(input("Quantidade de votos válidos? "))

# Calc
percentual_brancos = (votos_brancos / total_eleitores) * 100
percentual_nulos = (votos_nulos / total_eleitores) * 100
percentual_validos = (votos_validos / total_eleitores) * 100

# Print
print("Total de votos válidos: {:.2f}%".format(percentual_validos))
print("Total de votos em branco: {:.2f}%".format(percentual_brancos))
print("Total de votos nulos: {:.2f}%".format(percentual_nulos))
